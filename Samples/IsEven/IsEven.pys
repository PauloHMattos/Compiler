function IsEven(n : int) : bool
{
    return n % 2 == 0
}

function main()
{
    print("Digite quantos numeros você quer testar: ")
    var cnt = int(input())

    while (cnt > 0)
    {
        print("Digite um numero: ")
        var n = int(input())
        if IsEven(n)
        {
            print("É par")
        }
        else
        {
            print("Nao é par")
        }
        cnt -= 1
    }
}
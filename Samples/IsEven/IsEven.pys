function IsEven(n : int) : bool
{
    return n % 2 == 0
}

function main()
{
    while(true)
    {
        print("Digite um numero: ")
        var n = int(input())
        if IsEven(n)
        {
            print("É par")
        }
        else
        {
            print("Não é par")
        }
    }
}